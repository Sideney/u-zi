package finde;
public class Work3 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		double temp = 1;
		double e = 1;
		int i = 1;
		while (1 / temp >= 1e-6) {
			e = e + 1 / temp;
			i++;
			temp = temp * i;

		}
		System.out.println("The Value of e is " + e);
	}

}
